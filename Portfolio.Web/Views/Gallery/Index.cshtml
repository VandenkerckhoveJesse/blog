@model IEnumerable<String>
@{
    ViewData["Title"] = "Home Page";
}
<div id="gallery">
    @{ 
    foreach (string url in @Model)
    {
    <img src="@url" class="gallery-item" />
    }
    }
</div>

<style>
    #gallery {
        height: 100%;
        width: 100%;
        display: flex;
        flex-wrap: wrap;
        justify-content: center;
        align-items: center;
    }
    .gallery-item {
        padding: 10px;
        height: auto;
        width: auto;
        max-height: 500px;
        max-width: 800px;
        
    }

</style>



